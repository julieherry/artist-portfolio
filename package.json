    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.4.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^12.19.8",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-image-gallery": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-image-gallery": "^1.0.8",
    "react-images": "^1.1.7",
    "react-photo-gallery": "^8.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
